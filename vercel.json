{
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    }
  ],
  "functions": {
    "api/generate.py": {
      "maxDuration": 60
    },
    "api/scheduler.py": {
      "maxDuration": 60
    },
    "api/cron.py": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 12 * * *"
    }
  ]
} 